set number
syntax on
filetype plugin indent on
set title
set tabstop=8
set softtabstop=8
set textwidth=78
set shiftwidth=8
set noexpandtab
map gn :bn<cr>
map gp :bp<cr>
map gd :bd<cr>
map <C-J> :bnext<CR>
set wildmenu
set hidden
set showcmd
set ignorecase
set smartcase
set autoindent
set confirm
"set clipboard=unnamedplus"
set clipboard^=unnamed,unnamedplus
set nobackup
set nowritebackup
set noswapfile
set noundofile
